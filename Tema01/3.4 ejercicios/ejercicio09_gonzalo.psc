Algoritmo ejercicio09_gonzalo
	Definir r, h, volumen Como Real;
	Escribir "Por favor, introduzca el radio del cono en cm";
	Leer r;
	Escribir "Por favor, introduzca la altura del cono en cm";
	Leer h;
	r <- r * r;
	volumen <- (3.14 * r * h) / 3;
	Escribir "El volumen del cono es: ", volumen, "cm3";
FinAlgoritmo
