Algoritmo ejercicio01_gonzalo
	Definir numero1, numero2, multiplicacion Como Real;
	Escribir "Por favor, introduzca el primer número ";
	Leer numero1;
	Escribir "Por favor, introduzca el segundo número ";
	Leer numero2;
	multiplicacion <- numero1 * numero2;
	Escribir "El resultado es ", multiplicacion;
FinAlgoritmo